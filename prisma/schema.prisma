generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

// users
model User {
  id           Int        @id @default(autoincrement())
  createdAt    DateTime   @default(now())
  firstName    String
  lastName     String
  email        String     @unique
  phone        String
  isAdmin      Boolean    @default(false)

  // avatar is jpg or png base 64 data URI
  avatar       String     @default("data:image/png;base64,")

  // bcrypt hashed
  password     String

  // templates created by this user
  templates    Template[]

  // posts created by this user
  posts        Post[]

  // comments created by this user
  comments     Comment[]

  // reports created by this user
  reports      Report[]

  Rating Rating[]
}

// IMMUTABLE saved templates
model Template {
  id           Int        @id @default(autoincrement())
  createdAt    DateTime   @default(now())
  title        String
  explanation  String
  code         String

  // who created this template
  userId       Int
  user         User       @relation(fields: [userId], references: [id])

  // if this template is a fork of another template
  forkedFromId Int?
  forkedFrom   Template?  @relation("TemplateFork", fields: [forkedFromId], references: [id])

  // forks of this template
  forks        Template[] @relation("TemplateFork")

  // tags on this template
  tags         Tag[]

  // posts that reference this template
  posts        Post[]
}

model Post {
  id           Int        @id @default(autoincrement())
  createdAt    DateTime   @default(now())
  title        String
  description  String
  content      String?
  hidden       Boolean    @default(false)

  // who created this post
  userId       Int
  user         User       @relation(fields: [userId], references: [id])

  // tags on this post
  tags         Tag[]

  // comments on this post
  comments     Comment[]

  // templates this post links to
  templates    Template[]

  // reports of this post
  reports      Report[]

  Rating Rating[]
}

model Comment {
  id           Int        @id @default(autoincrement())
  createdAt    DateTime   @default(now())
  content      String
  hidden       Boolean    @default(false)

  // who created this comment
  userId       Int
  user         User       @relation(fields: [userId], references: [id])

  // what post is this comment on
  postId       Int
  post         Post       @relation(fields: [postId], references: [id])

  // if this comment is a reply to another comment
  parentId     Int?
  parent       Comment?   @relation("ParentComment", fields: [parentId], references: [id])

  // replies to this comment
  replies      Comment[]  @relation("ParentComment")

  // reports of this comment
  reports      Report[]
}

model Rating {
  id           Int        @id @default(autoincrement())
  createdAt    DateTime   @default(now())
  value        Int

  // who created this rating
  userId       Int
  user         User       @relation(fields: [userId], references: [id])

  // what post is this rating on
  postId       Int
  post         Post       @relation(fields: [postId], references: [id])
}

model Report {
  id           Int        @id @default(autoincrement())
  createdAt    DateTime   @default(now())
  reason       String

  // who created this report
  userId       Int
  user         User       @relation(fields: [userId], references: [id])

  // what comment is being reported
  commentId    Int?
  comment      Comment?   @relation(fields: [commentId], references: [id])

  // what post is being reported
  postId       Int?
  post         Post?      @relation(fields: [postId], references: [id])
}

model Tag {
  id           Int        @id @default(autoincrement())
  name         String     @unique

  // templates with this tag
  templates    Template[]

  // posts with this tag
  posts        Post[]
}
